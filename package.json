{
  "name": "ts-utils-srcs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec -- ts-node ../../scripts/build.ts",
    "commit": "git add --all && git-cz",
    "docs": "typedoc ./packages",
    "postinstall": "npm run bootstrap",
    "release": "lerna publish && npm run build && lerna exec -- npm publish dist --access public",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnc1997/ts-utils.git"
  },
  "author": "Thomas Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tnc1997/ts-utils/issues"
  },
  "homepage": "https://github.com/tnc1997/ts-utils#readme",
  "devDependencies": {
    "@angular/compiler": "^5.2.6",
    "@angular/compiler-cli": "^5.2.6",
    "@angular/core": "^5.2.6",
    "@types/jasmine": "^2.8.0",
    "@types/node": "^9.4.6",
    "commitizen": "^2.9.0",
    "cz-customizable": "^5.2.0",
    "jasmine-core": "^2.0.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-html-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.0",
    "karma-typescript": "^3.0.0",
    "karma-typescript-es6-transform": "^1.0.0",
    "lerna": "^2.9.0",
    "ng-packagr": "^2.1.0",
    "rxjs": "^5.5.6",
    "ts-node": "^5.0.0",
    "tsickle": "^0.27.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.0",
    "tslint-eslint-rules": "^5.1.0",
    "typedoc": "^0.11.0",
    "typescript": "~2.6.0",
    "zone.js": "^0.8.20"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
