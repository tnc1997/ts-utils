{
  "command": {
    "bootstrap": {
      "scope": [
        "@ts-utils/*"
      ],
      "hoist": true
    },
    "clean": {
      "yes": true
    },
    "exec": {
      "scope": [
        "@ts-utils/*"
      ]
    },
    "publish": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "forcePublish": "*",
      "message": "release: cut the %s release ***NO_CI***",
      "skipNpm": "true",
      "yes": true
    }
  },
  "lerna": "2.9.0",
  "packages": [
    "packages/*"
  ],
  "version": "1.0.0"
}
